{
    "name": "league/exo",
    "type": "composer-plugin",
    "description": "Creates a testing workbench.",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Pitt",
            "email": "cgpitt@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "composer-plugin-api": "1.*",
        "league/flysystem": "~1.0",
        "symfony/process": "~2.5",
        "roave/security-advisories": "dev-master@dev",
        "squizlabs/php_codesniffer": "~2.2",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.8"
    },
    "autoload": {
        "psr-4": {
            "Exo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Exo\\Tests\\": "tests"
        }
    },
    "extra": {
        "class": [
            "Exo\\Plugin"
        ]
    }
}
